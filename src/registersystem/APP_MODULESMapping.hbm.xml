<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="registersystem.APP_MODULESPOJO" table="APP_MODULES">
        <id column="MODULEID" name="MODULEID">
            <generator class="native"></generator>
        </id>
        <property column="ENABLED" name="ENABLED"></property>
        <property column="MODULENAME" name="MODULENAME"></property>
        <property column="VERSION" name="VERSION"></property>
    </class>
</hibernate-mapping>
